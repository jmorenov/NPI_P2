<PageFunction
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib" 
    x:Class="NPI_P2.FitnessScreen"
    x:TypeArguments="sys:Double"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    Loaded="PageLoaded"
    Title="NPI Fitness">

    <Grid Name="layoutGrid" Background="Black">
        <Image x:FieldModifier="public" Name="ImageSkeleton" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <Image Name="correct" Visibility="Hidden" HorizontalAlignment="Right" VerticalAlignment="Center" Source="Images/correct.png" Width="166" Height="163"/>
        <TextBox Text="" TextAlignment="Center" HorizontalAlignment="Right" VerticalAlignment="Bottom" Foreground="AliceBlue"  Background="Transparent" Name="time" Width="Auto" Height="Auto" FontFamily="KaiTi" FontSize="60"/>
    </Grid>
</PageFunction>
